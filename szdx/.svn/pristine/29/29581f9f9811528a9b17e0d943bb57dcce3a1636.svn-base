<figure class="highlight json"><table><tr><td class="gutter"><pre><span class="line">1</span><br><span class="line">2</span><br><span class="line">3</span><br><span class="line">4</span><br><span class="line">5</span><br><span class="line">6</span><br><span class="line">7</span><br><span class="line">8</span><br><span class="line">9</span><br><span class="line">10</span><br><span class="line">11</span><br><span class="line">12</span><br></pre></td><td class="code"><pre><span class="line">&#123;</span><br><span class="line">     <span class="attr">"description"</span>: <span class="string">"很好听的专辑"</span>,</span><br><span class="line">     <span class="attr">"name"</span>: <span class="string">"专辑一"</span>,</span><br><span class="line">     <span class="attr">"id"</span>: <span class="number">1</span>,</span><br><span class="line">     <span class="attr">"listeners"</span>: <span class="number">123</span>,</span><br><span class="line">     <span class="attr">"imageUrl"</span>: <span class="string">"http://p4.music.126.net/L0pZQaVogo1MmoJLO7SGIA==/1364493995720102.jpg?param=140y140"</span>,</span><br><span class="line">     <span class="attr">"songs"</span>: [</span><br><span class="line">     	<span class="string">"id"</span>: <span class="number">1</span>,</span><br><span class="line">     	<span class="string">"name"</span>: <span class="string">"歌曲1"</span></span><br><span class="line">     	<span class="string">"imageUrl"</span>: <span class="string">"http://p4.music.126.net/L0pZQaVogo1MmoJLO7SGIA==/1364493995720102.jpg?param=140y140"</span></span><br><span class="line">     ]</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>
