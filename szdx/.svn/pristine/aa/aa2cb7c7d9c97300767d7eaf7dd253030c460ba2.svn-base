
body {
    font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif;
    background: #F9F9F9;
}

@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1444110210_4838772.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/font_1444110210_4838772.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/font_1444110210_4838772.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/font_1444110210_4838772.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/font_1444110210_4838772.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont {
	font-family: "iconfont";
	font-style: normal;
}

.confirm input {
	width: 100%;
	height: 40px;
	border-radius: 3px;
	background: #6bacff;
	color: #FFF;
	font-size: 16px;
}
.confirm input:active {
	background: #2a60ae;
}

.warning-container {
	padding: 10px 20px;
	padding-top: 20px;
}
.warning-container .warning {
	width: 100%;
	height: 50px;
	background: #FFF;
	border-radius: 3px;
	text-align: center;
	line-height: 50px;
	font-size: 24px;
	color: #FF0000;
}

.header-wrapper {
	height: 200px;
	position: relative;
	overflow: hidden;
	background: -webkit-radial-gradient(left top, circle, #d1f0d0, #c3edda 10%, #84c4d4 40%, #a1bbdf);
	background: -ms-radial-gradient(left top, circle, #d1f0d0, #c3edda 10%, #84c4d4 40%, #a1bbdf);
	background: radial-gradient(left top, circle, #d1f0d0, #c3edda 10%, #84c4d4 40%, #a1bbdf);
}
.header-inner {
	height: 100%;
	position: relative;
}
.dashboard {
	position: absolute;
	width: 150px;
	height: 150px;
	text-align: center;
	border-radius: 50%;
	border: 4px solid rgba(255,255,255,.5);
   	background-image: url(http://cdn.www.wacxt.cn/res/images/wave2.png);
    background-repeat: no-repeat;
    background-position: bottom;
	background-color: rgba(255,255,255,.1);
	left: 50%;
	top: 50%;
	margin: -76px 0 0 -76px;
	z-index: 10;
    overflow: hidden;
	-webkit-background-size: 142px 142px;
	background-size: 142px 142px;
}
.number {
	margin-top: 38%;
	color: #fff;
	font-size: 34px;
	line-height: 1em;
	font-family:'custom';
	font-weight: 700;
}
.tip {
	color: #fff;
	font-size: 12px;
}
.waves {
	position: absolute;
	left: -1px;
	top: 0;
	width: 100%;
	height: 190px;
	z-index: 1;
	-webkit-transition: all 1s cubic-bezier(0.17, 0.89, 0.42, 1.89);
	transition: all 1s cubic-bezier(0.17, 0.89, 0.42, 1.89);

}
.waves .w1 {
	width: 800px;
	height: 30px;
	position: absolute;
	bottom: 10px;
	left: 50%;
	margin-left: -80px;
	background: url(http://cdn.www.wacxt.cn/res/images/w1.png) no-repeat;
	-webkit-background-size: auto 30px;
	background-size: auto 30px;
	-webkit-animation: wave1move 9s linear infinite;
	z-index: 10;
}
.waves .w2 {
	width: 700px;
	height: 28px;
	position: absolute;
	bottom: 20px;
	left: 50%;
	margin-left: -70px;
	background: url(http://cdn.www.wacxt.cn/res/images/w1.png) no-repeat;
	-webkit-background-size: auto 28px;
	background-size: auto 28px;
	-webkit-animation: wave2move 13s linear infinite;
	z-index: 1;

}
@-webkit-keyframes wave1move {
    0% {
        -webkit-transform: translate3d(0, 0, 0);
    }
    100% {
        -webkit-transform: translate3d(-365px, 0, 0);
    }
}
@-webkit-keyframes wave2move {
    0% {
        -webkit-transform: translate3d(0, 0, 0);
    }
    100% {
        -webkit-transform: translate3d(-333px, 0, 0);
    }
}

h4 {
	font-size: 20px;
	color: rgba(0, 0, 0, .3);
}
p.fee {
	color: rgba(0, 0, 0, .7);
	font-size: 42px;
	text-align: center;
}
span.decimal {
	font-size: 28px;
}
span.h1tips {
	font-size: 14px;
	letter-spacing: 0;
}
p.h1tips2 {
	color: rgba(0, 0, 0, .7);
	font-size: 24px;
	text-align: center;
	letter-spacing: 0;
    line-height: 110px;
}

.line1 {
	width: 100%;
	float: left;
	padding: 30px;
	padding-bottom: 0;
}
.line1 .yesterday {
	width: 50%;
	float: left;
	height: 150px;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #ccc;
}
.line1 .last {
	width: 50%;
	float: left;
	height: 150px;
	border-bottom: 1px solid #ccc;
	text-align: right;
}

.line2 {
	width: 100%;
	float: left;
	padding: 30px;
	padding-top: 0;
}
.line2 .yesterday {
	width: 50%;
	float: left;
	height: 150px;
	border-right: 1px solid #ccc;
}
.line2 .last {
	width: 50%;
	float: left;
	height: 150px;
	padding: 20px;
}
.line2 .last .h1tips {
	text-align: center;
	font-size: 20px;
	color: rgba(0, 0, 0, .7);
	line-height: 20px;
}
.line2 .last:active {
	background: #eee;
}

.hide {
	display: none;
}

.day-container {
	max-width: 100%;
	background: #FFF;
}
.days-container {
	padding-top: 10px;
	max-width: 100%;
	overflow: hidden;
    margin-bottom: 10px;
}

.tools {
	width: 100%;
	float: left;
	padding: 10px 15px;
	background: #FFF;
	border-bottom: 1px solid #ddd;
	font-size: 18px;
	line-height: 30px;
	color: #222;
}
.tools-lowfee-warn {
	float: left;
}

.tools-button {
	float: right;
	height: 30px;
	width: 60px;
	border-radius: 15px;
	background: #ddd;
}
.tools-button:after {
	display: block;
	content: "";
	background: #FFF;
	width: 24px;
	height: 24px;
	border-radius: 50%;
	margin-top: 3px;
	margin-left: 3px;
	pointer-events: none;
}

.enable {
	float: right;
	height: 30px;
	width: 60px;
	border-radius: 15px;
	background: #7dd17d;
}
.enable:after {
	display: block;
	content: "";
	background: #FFF;
	width: 24px;
	height: 24px;
	border-radius: 50%;
	margin-top: 3px;
	margin-left: 33px;
	pointer-events: none;
}

.heightB {
    float: left;
	width: 100%;
    height: 10px;
    margin-top: 20px;
}
.heightB p {
	text-align: center;
    color: rgba(0, 0, 0, .3);
    font-size: 12px;
}

.navbar {
	position: fixed;
	bottom: 0;
	left: 0;
	height: 50px;
	width: 100%;
	background: #FFF;
	color: #222;
	font-size: 18px;
	border-top: 1px solid #ddd;
}
.navbar .day {
	width: 50%;
	float: left;
	text-align: right;
	padding-right: 12%;
	line-height: 50px;
}
.navbar .days {
	width: 50%;
	float: left;
	text-align: left;
	padding-left: 12%;
	line-height: 50px;
}
.navbar div:active {
	background: #eee;
}
.navbar .active {
	font-weight: bolder;
	color: #6bacff;
}