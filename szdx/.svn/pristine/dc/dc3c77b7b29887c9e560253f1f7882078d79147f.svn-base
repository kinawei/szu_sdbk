@charset 'utf-8';
* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    -webkit-tap-highlight-color: transparent;
    -webkit-appearance: none
}

body, html {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
    text-decoration: none
}

html {
    text-decoration: none;
    background-color: #f0f0f0
}

body {
    font-family: Helvetica, Verdana, Arial, sans-serif;
    font-size: 14px;
    line-height: 1.6;
    text-decoration: none;
    word-wrap: break-word;
    -webkit-font-smoothing: antialiased
}

h1, h2, h3, h4, h5, h6 {
    line-height: 2.6
}

form {
    margin: 0
}

.inline {
    display: inline
}

a {
    text-decoration: none;
    color: #8b0a42;
    -webkit-tap-highlight-color: transparent;
    -webkit-appearance: none
}

a:hover {
    text-decoration: none;
    color: #5e032a
}

a.light {
    text-decoration: none;
    color: #fff
}

a.light:hover {
    text-decoration: none;
    color: #ddd
}

.clear {
    clear: both
}
.page_title{
	position: relative;
    top: -40px;
    font-size: 0.8rem;
    text-align: center;
}
.book_link{
	display: block;
    border-bottom: 1px solid #e4e4e4;
    width: 100%;
    line-height: 30px;
    font-size: 20px;
    padding-left: 10px;
}
.book_link:last-child{
    border: 0;
}

.btn_get{
	width: 300px;
    height: 50px;
    border-radius: 50px;
    font-size: 20px;
    line-height: 50px;
    text-align: center;
    border: 1px solid #aaaaaa;
    margin: 10px auto;
}
.b_title{  
    font-size: 16px;
  line-height: 40px;
  font-weight: bold;
}
.xsb{
	position: relative;
    top: -40px;
    font-size: 0.8rem;
    text-align: center;
    color: green;
}
.welcome{
	position: relative;
    top: -20px;
    font-size: 0.6rem;
}
.sy{
    color: #333;
  font-size: 12px;
  margin-left: 10px;
}