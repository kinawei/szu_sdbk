.bgimg{position:fixed;top:0;left:0;width:100%;height:100%;background-image:-webkit-linear-gradient(290deg,#21b56a,#408ec0);background-image:linear-gradient(160deg,#21b56a,#408ec0);z-index:-1000}.userInfo{float:left;width:100%}.userInfo .headImg{width:110px;height:110px;padding:5px;margin:20px auto;margin-bottom:5px;border-radius:50%;background:hsla(0,0%,100%,.05)}.userInfo .headImg .img{width:100px;height:100px;border-radius:50%;border:5px solid hsla(0,0%,100%,.1)}.userInfo .headImg .img img{width:90px;height:90px;border-radius:50%;display:block}.userInfo h3{width:100%;margin-bottom:15px;color:#fff;font-size:20px;line-height:30px;font-weight:400;text-align:center;display:block}.apps{float:left;width:100%}.apps .sourceTips{padding-left:10px;font-size:14px;color:#fff}.apps .appContainer{float:left;width:50%}.apps .appContainer:nth-child(2n){padding:5px 5px 5px 10px}.apps .appContainer:nth-child(2n-1){padding:5px 10px 5px 5px}.apps .appContainer .app{width:100%;height:150px;padding:20px;border-radius:3px;box-shadow:0 3px 10px rgba(0,0,0,.1);background:hsla(0,0%,100%,.98);-webkit-transition:all 233ms ease;transition:all 233ms ease}.apps .appContainer .app .appIcon{width:55px;margin:14px auto;display:block}.apps .appContainer .app h4{color:#272636;font-size:16px;line-height:20px;text-align:center}.apps .appContainer .app:active{background:hsla(0,0%,100%,.8)}.footer{float:left;width:100%;height:50px;margin-top:10px;font-size:12px;color:hsla(0,0%,100%,.8);text-align:center}.fade-transition{-webkit-transition:opacity .3s ease;transition:opacity .3s ease;opacity:1}.fade-enter,.fade-leave{opacity:0}.v-spinner{position:fixed;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}@-webkit-keyframes v-gridStretchDelay{0%{-webkit-transform:scale(1);transform:scale(1)}50%{-webkit-transform:scale(.5);transform:scale(.5);opacity:.7}to{-webkit-transform:scale(1);transform:scale(1);opacity:1}}@keyframes v-gridStretchDelay{0%{-webkit-transform:scale(1);transform:scale(1)}50%{-webkit-transform:scale(.5);transform:scale(.5);opacity:.7}to{-webkit-transform:scale(1);transform:scale(1);opacity:1}}